..std::linux::{str::String, io::println};

A {
    v: U64
} {
    new(v: U64) -> A {
        -> A { v: v };
    }

    to_string(self: A) -> String {
        -> self.v.to_string();
    }

    +(self: A, other: A) -> A {
        -> A { v: self.v + other.v };
    }
}

>_start() {
    a: A = A::new(1);
    b: A = A::new(2);

    println((a + b).to_string());
}
