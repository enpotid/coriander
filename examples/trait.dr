nec std;
index std::{ops::Add, linux::{str::String, io::println, fmt::{FmtKind, Print}}};

A {
    v: usize
} {
    new(v: usize) -> A {
        return A { v };
    }
}

.A in Add {
    add(self, other: A) -> A {
        return A { v: self.v + other.v };
    }
}

.A in Print {
    print(self) -> String {
        return self.v.to_string();
    }
}

>main() {
    let a = A::new(1);
    let b = A::new(2);

    let ans = format(FmtKind::Display, a + b);
    println(ans);
}
